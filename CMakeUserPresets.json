{
    "version": 4,
    "vendor": {
        "conan": {}
    },
    "include": [
        "build\\generators\\CMakePresets.json"
    ],
    "configurePresets": [ 
        {
            "name": "default",
            "inherits": ["conan-default"],
            "installDir": "${sourceDir}/binaries",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/binaries"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "debug",
            "configurePreset": "conan-default",
            "configuration": "Debug",
            "jobs": 32
        },
        {
            "name": "development",
            "configurePreset": "conan-default",
            "configuration": "RelWithDebInfo",
            "jobs": 32
        },
        {
            "name": "release",
            "configurePreset": "conan-default",
            "configuration": "Release",
            "jobs": 32
        }
    ]
}